doctype html
html
  head
    link(rel='stylesheet', href='/styles.css')
  body
    h1= title
    div(class="tbl-header")
      table(cellpadding="0" cellspacing="0" border="0")
        thead
          tr
            th Name
            th Count
    div(class="tbl-content")
      table(cellpadding="0" cellspacing="0" border="0")
        tbody
          each val, key in data
            tr
              td
                a(href='/data?characterName=' + key)= key
              td= val